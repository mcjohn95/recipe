<h1>Listing special_recipes</h1>

<table>
  <thead>
    <tr>
      <th>Diet</th>
      <th>Mealrecipe</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @special_recipes.each do |special_recipe| %>
      <tr>
        <td><%= special_recipe.Diet_id %></td>
        <td><%= special_recipe.mealRecipe_id %></td>
        <td><%= link_to 'Show', special_recipe %></td>
        <td><%= link_to 'Edit', edit_special_recipe_path(special_recipe) %></td>
        <td><%= link_to 'Destroy', special_recipe, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Special recipe', new_special_recipe_path %>
